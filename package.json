{
  "name": "im-desktop",
  "productName": "im-desktop",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts ."
  },
  "keywords": [],
  "author": {
    "name": "mclarck",
    "email": "contact.mclarck@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "im_desktop"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/screens/main/index.html",
                  "js": "./src/screens/main/renderer.ts",
                  "name": "main_window"
                },
                {
                  "html": "./src/screens/newstock/index.html",
                  "js": "./src/screens/newstock/renderer.ts",
                  "name": "newstock_window"
                },
                {
                  "html": "./src/screens/settings/index.html",
                  "js": "./src/screens/settings/renderer.ts",
                  "name": "settings_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@apollo/client": "^3.3.11",
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "@electron-forge/plugin-webpack": "6.0.0-beta.54",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@types/graphql": "^14.5.0",
    "@types/howler": "^2.2.1",
    "@types/i18n-js": "^3.8.0",
    "@types/knex": "^0.16.1",
    "@types/leaflet": "^1.5.23",
    "@types/leaflet-geosearch": "^2.7.1",
    "@types/lodash": "^4.14.168",
    "@types/mathjs": "^6.0.11",
    "@types/node-sass": "^4",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-icons": "^3.0.0",
    "@types/react-leaflet": "^2.5.2",
    "@types/react-router-dom": "^5.1.7",
    "@types/sass": "^1.16.0",
    "@types/socket.io-client": "^1.4.35",
    "@types/sqlite3": "^3.1.7",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "css-loader": "^4.2.1",
    "electron": "11.3.0",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "^2.20.0",
    "fork-ts-checker-webpack-plugin": "^5.0.14",
    "graphql": "^15.5.0",
    "howler": "^2.2.1",
    "i18n-js": "^3.8.0",
    "knex": "^0.21.18",
    "leaflet": "^1.7.1",
    "leaflet-geosearch": "^3.2.1",
    "lodash": "^4.17.21",
    "mathjs": "^9.2.0",
    "node-loader": "^1.0.1",
    "node-sass": "^4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^6.15.3",
    "react-icons": "^4.2.0",
    "react-leaflet": "^3.1.0",
    "react-router-dom": "^5.2.0",
    "sass": "^1.32.8",
    "serialize-javascript": "^5.0.1",
    "socket.io-client": "^3.1.1",
    "sqlite3": "^5.0.2",
    "style-loader": "^1.2.1",
    "ts-loader": "^8.0.2",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0"
  }
}
